{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS",
        "runtimeExecutable": "yarn",
        "runtimeArgs": ["run", "start:debug", "--", "--inspect=9229"],
        "skipFiles": ["<node_internals>/**"],
        "sourceMaps": true,
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "console": "integratedTerminal"
      }
    ]
}